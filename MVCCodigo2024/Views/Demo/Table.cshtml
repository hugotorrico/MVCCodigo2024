
@{
    ViewBag.Title = "Table";
}

@{
    var fruits = new List<string> {
        "Apple", "Banana", "Cherry","Orange" };
}



<h2>Lista de Productos</h2>

@{ var cont = 0;}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Cantidad</th>
        </tr>
    </thead>
    <tbody>

       
        @foreach (var item in fruits)
        {
            cont = cont + 1;

            <tr>
                <td>@cont</td>
                <td>@item</td>
                <td>25</td>
                <td>10</td>
            </tr>
        }
   
    </tbody>
</table>


